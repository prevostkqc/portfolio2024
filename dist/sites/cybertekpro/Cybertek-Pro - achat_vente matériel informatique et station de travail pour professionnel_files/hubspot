function generateForm(n){window.onscroll=function(){window.scrollTo(scrollLeft,scrollTop)};let r=document.createElement("div");r.classList.add("cbk_hbspt__background");document.body.appendChild(r);n();let t=document.createElement("div");t.classList.add("cbk_hbspt__close-btn-wrapper");document.body.appendChild(t);let i=document.createElement("img");i.classList.add("cbk_hbspt__close-img");i.src="/Content/Images/icons/icn_cross.svg";t.appendChild(i);t.appendChild(document.querySelector(".hbspt-form"));i.addEventListener("click",()=>{document.querySelector(".hbspt-form").remove(),document.querySelector(".cbk_hbspt__background").remove(),t.remove(),window.onscroll=function(){}})}let scrollTop=window.pageYOffset||document.body.scrollTop,scrollLeft=window.pageXOffset||document.body.scrollLeft;const hbsptDevisBtn=document.querySelectorAll(".cpro_hbspt__btn--devis"),hbsptDemandeBtn=document.querySelectorAll(".cpro_hbspt__btn--demande");hbsptDevisBtn.forEach(n=>n.addEventListener("click",function(){generateForm(devisForm)}));hbsptDemandeBtn.forEach(n=>n.addEventListener("click",function(){generateForm(demandeForm)}));const devisForm=()=>{hbspt.forms.create({region:"eu1",portalId:"143332020",formId:"0f26bb4b-fde7-4329-bc75-dd32d306cc18"})},demandeForm=()=>{hbspt.forms.create({region:"eu1",portalId:"143332020",formId:"a562ee79-774e-4f24-9219-91ed15d8fca7"})}