(function($){
    $.fn.caret = function(options, opt2) {
        var start, end, t = this[0],
            isIE = typeof document.selection !== 'undefined';
        if (typeof options === "object" && typeof options.start === "number" && typeof options.end === "number") {
            start = options.start;
            end = options.end;
        } else if (typeof options === "number" && typeof opt2 === "number") {
            start = options;
            end = opt2;
        } else if (typeof options === "string") {
            if ((start = t.value.indexOf(options)) > -1) {
                end = start + options[len];
            } else {
                start = null;
            }
        } else if (Object.prototype.toString.call(options) === "[object RegExp]") {
            var re = options.exec(t.value);
            if (re != null) {
                start = re.index;
                end = start + re[0][len];
            }
        }
        if (typeof start != "undefined") {
            if (isIE) {
                var selRange = this[0].createTextRange();
                selRange.collapse(true);
                selRange.moveStart('character', start);
                selRange.moveEnd('character', end - start);
                selRange.select();
            } else {
                this[0].selectionStart = start;
                this[0].selectionEnd = end;
            }
            this[0].focus();
            return this
        } else {
            // Modification as suggested by Андрей Юткин
            if (isIE) {
                var selection = document.selection;
                if (this[0].tagName.toLowerCase() != "textarea") {
                    var val = this.val(),
                        range = selection[createRange]()[duplicate]();
                    range.moveEnd("character", val[len]);
                    var s = (range.text == "" ? val[len] : val.lastIndexOf(range.text));
                    range = selection[createRange]()[duplicate]();
                    range.moveStart("character", -val[len]);
                    var e = range.text[len];
                } else {
                    var range = selection[createRange](),
                        stored_range = range[duplicate]();
                    stored_range.moveToElementText(this[0]);
                    stored_range.setEndPoint('EndToEnd', range);
                    var s = stored_range.text[len] - range.text[len],
                        e = s + range.text[len]
                }
                // End of Modification
            } else {
                var s = t.selectionStart,
                    e = t.selectionEnd;
            }
            var te = t.value.substring(s, e);
            return {
                start: s,
                end: e,
                text: te,
                replace: function(st) {
                    return t.value.substring(0, s) + st + t.value.substring(e, t.value[len])
                }
            }
        }
    }
})(jQuery);
(function($) {
    $.openPopin = function(params) {
        params = $.extend({
            type : 'ajax',
            url : '',
            target : '#popin-0',
            closable : true
        }, params);
        
        if(params.type == 'ajax') {
            $('<a data-toggle="modal" href="' + params.url + '" data-target="' + params.target + '" ' + (params.closable !== true ? 'data-closable="false"' : '') + ' style="display:none"></a>').appendTo('body').click().remove();
        }
        else if(params.type == 'inline') {
            $('<a data-toggle="modal" data-target="' + params.target + '" ' + (params.closable !== true ? 'data-closable="false"' : '') + ' style="display:none"></a>').appendTo('body').click().remove();
        }
    }

    $.popinUniqID = function(num) {
        if(num === undefined || num === null)
            num = 0;

        var popinId = '#popin-' + num;
        if($(popinId).length == 0)
            return popinId;
        else
            return $.popinUniqID(num + 1);
    }
})(jQuery);

// Make sure the DOM elements are loaded and accounted for
$(document).ready(function() {

  // Match to Bootstraps data-toggle for the modal
  // and attach an onclick event handler
  $(document).on('click', '[data-toggle="modal"]', function(e) {

    // From the clicked element, get the data-target arrtibute
    // which BS3 uses to determine the target modal
    var target_modal = $(e.currentTarget).data('target');
    // also get the remote content's URL
    var remote_content = $(e.currentTarget).attr('href');

    if(target_modal == undefined){
        if(remote_content.substr(0, 1) == '#') {
            target_modal = remote_content;
        }
        else{
            target_modal = $.popinUniqID();
        }
    }
    else if(remote_content == undefined)
        remote_content = target_modal;

    // Find the target modal in the DOM
    var modal = $(target_modal);
    // Find the modal's <div class="modal-content"> so we can populate it
    var modalContent = $(target_modal + ' .modal-content');
    // Closable ?
    var closable = $(e.currentTarget).data('closable') !== false;

    var modalCallbackContent = function(modalContent, closable) {
        if(!closable)
            modalContent.find('.modal-header .close').remove();
    };

    if(remote_content != '' && remote_content.substr(0, 1) != '#') {
        // Cas de la popin AJAX

        if(modalContent.length !== 0)
            modalContent.remove();

        $('body').append('<div id="' + target_modal.substr(1) + '" class="modal fade" role="dialog" arian-hidden="true" ' + (!closable ? 'data-backdrop="static" data-keyboard="false"' : '') + '><div class="modal-dialog"><div class="modal-content"></div></div></div>');
        modal = $(target_modal);
        modalContent = $(target_modal + ' .modal-content');

        modal.find('.modal-dialog').addClass($(e.currentTarget).data('class'));

        // Capture BS3's show.bs.modal which is fires
        // immediately when, you guessed it, the show instance method
        // for the modal is called
        modal.on('shown.bs.modal', function () {
            // use your remote content URL to load the modal body
            modalContent.load(remote_content, function(){
                modalCallbackContent(modalContent, closable);
            });
        }).on('hidden.bs.modal', function(){
            modal.remove();
        }).modal();

        if($(e.currentTarget).is('label')) {
            $(e.currentTarget).prev('input[type="radio"]').prop('checked', 'checked')
        }
    }
    else if(remote_content.substr(0, 1) == '#') {
        // Cas de la popin INLINE
        modalCallbackContent(modalContent, closable);
    }

    e.stopPropagation();
    return false;
  });
});
(function($){
    $.topbarOpenLayer = function(layer){
        $('nav#topBar .layer.active:not(.layer-' + layer +'), nav#topBar .link-layer.active:not(.link-' + layer +')').removeClass('active');
        $('nav#topBar .layer.layer-' + layer + ',nav#topBar .link-layer.link-' + layer).toggleClass('active');

        if($('nav#topBar .layer.layer-' + layer).hasClass('active'))
        {
            if($('#topbar-overlay').size() == 0)
            {
                $('body').append('<div id="topbar-overlay" />');
                $('#topbar-overlay').on('click', function(){
                    $.topbarOpenLayer();
                }).stop().hide().fadeIn('fast');
            }
        }
        else
            $('#topbar-overlay').stop().fadeOut('fast', function(){
                $(this).remove();
            });
    }

    $.topBarTogglePasswordVisibility = function(){
        if($('#topbar-password-field').is('input[type="password"]'))
            $('#topbar-password-field').attr('type', 'text');
        else
            $('#topbar-password-field').attr('type', 'password');
    }
})(jQuery);
(function($){
	$.support.inputDate = function(){
		var input = document.createElement('input');
		input.setAttribute('type','date');

		var notADateValue = 'not-a-date';
		input.setAttribute('value', notADateValue); 

		return !(input.value === notADateValue);
	}();

	$.support.inputMonth = function(){
		var input = document.createElement('input');
		input.setAttribute('type','month');

		var notAMonthValue = 'not-a-month';
		input.setAttribute('value', notAMonthValue); 

		return !(input.value === notAMonthValue);
	}();
})(jQuery);

}
