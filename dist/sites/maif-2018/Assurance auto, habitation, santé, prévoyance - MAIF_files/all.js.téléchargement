){var d,e,f,g,j,h=null,i=this;return b=b||!1,i.animating===!0&&i.options.waitForAnimate===!0||i.options.fade===!0&&i.currentSlide===a||i.slideCount<=i.options.slidesToShow?void 0:(b===!1&&i.asNavFor(a),d=a,h=i.getLeft(d),g=i.getLeft(i.currentSlide),i.currentLeft=null===i.swipeLeft?g:i.swipeLeft,i.options.infinite===!1&&i.options.centerMode===!1&&(0>a||a>i.getDotCount()*i.options.slidesToScroll)?void(i.options.fade===!1&&(d=i.currentSlide,c!==!0?i.animateSlide(g,function(){i.postSlide(d)}):i.postSlide(d))):i.options.infinite===!1&&i.options.centerMode===!0&&(0>a||a>i.slideCount-i.options.slidesToScroll)?void(i.options.fade===!1&&(d=i.currentSlide,c!==!0?i.animateSlide(g,function(){i.postSlide(d)}):i.postSlide(d))):(i.options.autoplay&&clearInterval(i.autoPlayTimer),e=0>d?i.slideCount%i.options.slidesToScroll!==0?i.slideCount-i.slideCount%i.options.slidesToScroll:i.slideCount+d:d>=i.slideCount?i.slideCount%i.options.slidesToScroll!==0?0:d-i.slideCount:d,i.animating=!0,i.$slider.trigger("beforeChange",[i,i.currentSlide,e]),f=i.currentSlide,i.currentSlide=e,i.setSlideClasses(i.currentSlide),i.options.asNavFor&&(j=i.getNavTarget(),j=j.slick("getSlick"),j.slideCount<=j.options.slidesToShow&&j.setSlideClasses(i.currentSlide)),i.updateDots(),i.updateArrows(),i.options.fade===!0?(c!==!0?(i.fadeSlideOut(f),i.fadeSlide(e,function(){i.postSlide(e)})):i.postSlide(e),void i.animateHeight()):void(c!==!0?i.animateSlide(h,function(){i.postSlide(e)}):i.postSlide(e))))},b.prototype.startLoad=function(){var a=this;a.options.arrows===!0&&a.slideCount>a.options.slidesToShow&&(a.$prevArrow.hide(),a.$nextArrow.hide()),a.options.dots===!0&&a.slideCount>a.options.slidesToShow&&a.$dots.hide(),a.$slider.addClass("slick-loading")},b.prototype.swipeDirection=function(){var a,b,c,d,e=this;return a=e.touchObject.startX-e.touchObject.curX,b=e.touchObject.startY-e.touchObject.curY,c=Math.atan2(b,a),d=Math.round(180*c/Math.PI),0>d&&(d=360-Math.abs(d)),45>=d&&d>=0?e.options.rtl===!1?"left":"right":360>=d&&d>=315?e.options.rtl===!1?"left":"right":d>=135&&225>=d?e.options.rtl===!1?"right":"left":e.options.verticalSwiping===!0?d>=35&&135>=d?"down":"up":"vertical"},b.prototype.swipeEnd=function(a){var c,d,b=this;if(b.dragging=!1,b.interrupted=!1,b.shouldClick=b.touchObject.swipeLength>10?!1:!0,void 0===b.touchObject.curX)return!1;if(b.touchObject.edgeHit===!0&&b.$slider.trigger("edge",[b,b.swipeDirection()]),b.touchObject.swipeLength>=b.touchObject.minSwipe){switch(d=b.swipeDirection()){case"left":case"down":c=b.options.swipeToSlide?b.checkNavigable(b.currentSlide+b.getSlideCount()):b.currentSlide+b.getSlideCount(),b.currentDirection=0;break;case"right":case"up":c=b.options.swipeToSlide?b.checkNavigable(b.currentSlide-b.getSlideCount()):b.currentSlide-b.getSlideCount(),b.currentDirection=1}"vertical"!=d&&(b.slideHandler(c),b.touchObject={},b.$slider.trigger("swipe",[b,d]))}else b.touchObject.startX!==b.touchObject.curX&&(b.slideHandler(b.currentSlide),b.touchObject={})},b.prototype.swipeHandler=function(a){var b=this;if(!(b.options.swipe===!1||"ontouchend"in document&&b.options.swipe===!1||b.options.draggable===!1&&-1!==a.type.indexOf("mouse")))switch(b.touchObject.fingerCount=a.originalEvent&&void 0!==a.originalEvent.touches?a.originalEvent.touches.length:1,b.touchObject.minSwipe=b.listWidth/b.options.touchThreshold,b.options.verticalSwiping===!0&&(b.touchObject.minSwipe=b.listHeight/b.options.touchThreshold),a.data.action){case"start":b.swipeStart(a);break;case"move":b.swipeMove(a);break;case"end":b.swipeEnd(a)}},b.prototype.swipeMove=function(a){var d,e,f,g,h,b=this;return h=void 0!==a.originalEvent?a.originalEvent.touches:null,!b.dragging||h&&1!==h.length?!1:(d=b.getLeft(b.currentSlide),b.touchObject.curX=void 0!==h?h[0].pageX:a.clientX,b.touchObject.curY=void 0!==h?h[0].pageY:a.clientY,b.touchObject.swipeLength=Math.round(Math.sqrt(Math.pow(b.touchObject.curX-b.touchObject.startX,2))),b.options.verticalSwiping===!0&&(b.touchObject.swipeLength=Math.round(Math.sqrt(Math.pow(b.touchObject.curY-b.touchObject.startY,2)))),e=b.swipeDirection(),"vertical"!==e?(void 0!==a.originalEvent&&b.touchObject.swipeLength>4&&a.preventDefault(),g=(b.options.rtl===!1?1:-1)*(b.touchObject.curX>b.touchObject.startX?1:-1),b.options.verticalSwiping===!0&&(g=b.touchObject.curY>b.touchObject.startY?1:-1),f=b.touchObject.swipeLength,b.touchObject.edgeHit=!1,b.options.infinite===!1&&(0===b.currentSlide&&"right"===e||b.currentSlide>=b.getDotCount()&&"left"===e)&&(f=b.touchObject.swipeLength*b.options.edgeFriction,b.touchObject.edgeHit=!0),b.options.vertical===!1?b.swipeLeft=d+f*g:b.swipeLeft=d+f*(b.$list.height()/b.listWidth)*g,b.options.verticalSwiping===!0&&(b.swipeLeft=d+f*g),b.options.fade===!0||b.options.touchMove===!1?!1:b.animating===!0?(b.swipeLeft=null,!1):void b.setCSS(b.swipeLeft)):void 0)},b.prototype.swipeStart=function(a){var c,b=this;return b.interrupted=!0,1!==b.touchObject.fingerCount||b.slideCount<=b.options.slidesToShow?(b.touchObject={},!1):(void 0!==a.originalEvent&&void 0!==a.originalEvent.touches&&(c=a.originalEvent.touches[0]),b.touchObject.startX=b.touchObject.curX=void 0!==c?c.pageX:a.clientX,b.touchObject.startY=b.touchObject.curY=void 0!==c?c.pageY:a.clientY,void(b.dragging=!0))},b.prototype.unfilterSlides=b.prototype.slickUnfilter=function(){var a=this;null!==a.$slidesCache&&(a.unload(),a.$slideTrack.children(this.options.slide).detach(),a.$slidesCache.appendTo(a.$slideTrack),a.reinit())},b.prototype.unload=function(){var b=this;a(".slick-cloned",b.$slider).remove(),b.$dots&&b.$dots.remove(),b.$prevArrow&&b.htmlExpr.test(b.options.prevArrow)&&b.$prevArrow.remove(),b.$nextArrow&&b.htmlExpr.test(b.options.nextArrow)&&b.$nextArrow.remove(),b.$slides.removeClass("slick-slide slick-active slick-visible slick-current").attr("aria-hidden","true").css("width","")},b.prototype.unslick=function(a){var b=this;b.$slider.trigger("unslick",[b,a]),b.destroy()},b.prototype.updateArrows=function(){var b,a=this;b=Math.floor(a.options.slidesToShow/2),a.options.arrows===!0&&a.slideCount>a.options.slidesToShow&&!a.options.infinite&&(a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false"),a.$nextArrow.removeClass("slick-disabled").attr("aria-disabled","false"),0===a.currentSlide?(a.$prevArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$nextArrow.removeClass("slick-disabled").attr("aria-disabled","false")):a.currentSlide>=a.slideCount-a.options.slidesToShow&&a.options.centerMode===!1?(a.$nextArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")):a.currentSlide>=a.slideCount-1&&a.options.centerMode===!0&&(a.$nextArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")))},b.prototype.updateDots=function(){var a=this;null!==a.$dots&&(a.$dots.find("li").removeClass("slick-active").attr("aria-hidden","true"),a.$dots.find("li").eq(Math.floor(a.currentSlide/a.options.slidesToScroll)).addClass("slick-active").attr("aria-hidden","false"))},b.prototype.visibility=function(){var a=this;a.options.autoplay&&(document[a.hidden]?a.interrupted=!0:a.interrupted=!1)},a.fn.slick=function(){var f,g,a=this,c=arguments[0],d=Array.prototype.slice.call(arguments,1),e=a.length;for(f=0;e>f;f++)if("object"==typeof c||"undefined"==typeof c?a[f].slick=new b(a[f],c):g=a[f].slick[c].apply(a[f].slick,d),"undefined"!=typeof g)return g;return a}});
(function($) {
    $.fn.affixAbsolute = function() {
        return this.each(function() {
            var elm = $(this);
            var parent = elm.closest($(this).data('parent'));
            var container = elm.closest($(this).data('container'));
            process(elm, parent, container);

            $(document).off('scroll.affixAbsolute').on('scroll.affixAbsolute', function() {
                process(elm, parent, container);
            });
        });

        function process(elm, parent, container) {
            if (container.offset().top + container.height() <= $(document).scrollTop() + elm.outerHeight(true)) {
                elm.removeClass('affix-top').addClass('affix-abs affix-bottom');
                elm.css('top', container.offset().top - parent.offset().top + container.height() - elm.outerHeight(true));
            } else {
                if (parent.offset().top > $(document).scrollTop()) {
                    elm.removeClass('affix-abs affix-bottom').addClass('affix-top');
                } else {
                    elm.removeClass('affix-top affix-bottom').addClass('affix-abs');
                    elm.css('top', $(document).scrollTop() - parent.offset().top);
                }
            }
        }
    }
})(jQuery);

$(document).ready(function() {
    $('[data-spy="affix-absolute"]').affixAbsolute();
});

$(document).ready(function() {
    var lastScrollTop = 0;
    $(window).scroll(function(event) {
        var st = $(this).scrollTop();
        if (st < lastScrollTop) {
            // downscroll code
            $('.floating-contact').removeClass('slide-hidden');
        } else {
            // upscroll code
            $('.floating-contact').addClass('slide-hidden');
        }
        lastScrollTop = st;
    });
});

(function($){
	$.modEditDataVisualisationLoad = function()
	{
		$('.mod-edito-data-visualisation .animation:not(.animate)').each(function(){
			var visible = $(this).offset().top > $(window).scrollTop() &&  $(this).offset().top < $(window).scrollTop() + $(window).height();
			var $this = $(this);
			if(visible){
				$(this).addClass('animate');
				setTimeout(function(){
					$this.trigger('load');
				}, 1000);
			}
		});
	}

	$.fn.modEditDataVisualisation = function(percent)
	{
		return this.each(function()
		{
			var picto = $(this).find('.picto-anime');

			if(picto.is('.animation-conducteur'))
				picto.find('.layer-2').height(percent + '%');
			else if(picto.is('.animation-cercle'))
				picto.find('.circle-stroke').css('stroke-dashoffset', (1 - percent / 100) * 157);
			else if(picto.is('.animation-population')){
				var duration = 1000;
				picto.find('.maificon').slice(0, Math.round(percent / 10)).each(function(){
					var $this = $(this);
					setTimeout(function(){
						$this.addClass('active');
					}, $this.index() * duration / 10);
				});
			}
			else if(picto.is('.animation-jauge'))
				picto.find('.jauge').css('top', -percent + '%');
		});
	}
})(jQuery);

$(document).ready(function(){
	$(window).scroll(function(){
		$.modEditDataVisualisationLoad();
	});
	$.modEditDataVisualisationLoad();
});
Slider.prototype.setValue = function(val, triggerSlideEvent, triggerChangeEvent) {
	if (!val) {
		val = 0;
	}
	var oldValue = this.getValue();
	this.options.value = this._validateInputValue(val);
	var applyPrecision = this._applyPrecision.bind(this);

	if (this.options.range) {
		this.options.value[0] = applyPrecision(this.options.value[0]);
		this.options.value[1] = applyPrecision(this.options.value[1]);

		this.options.value[0] = Math.max(this.options.min, Math.min(this.options.max, this.options.value[0]));
		this.options.value[1] = Math.max(this.options.min, Math.min(this.options.max, this.options.value[1]));
	} else {
		this.options.value = applyPrecision(this.options.value);
		this.options.value = [ Math.max(this.options.min, Math.min(this.options.max, this.options.value))];
		this._addClass(this.handle2, 'hide');
		if (this.options.selection === 'after') {
			this.options.value[1] = this.options.max;
		} else {
			this.options.value[1] = this.options.min;
		}
	}

	if (this.options.max > this.options.min) {
		this.percentage = [
			this._toPercentage(this.options.value[0]),
			this._toPercentage(this.options.value[1]),
			this.options.step * 100 / (this.options.max - this.options.min)
		];
	} else {
		this.percentage = [0, 0, 100];
	}

	this._layout();
	var newValue = this.options.range ? this.options.value : this.options.value[0];
	this._setDataVal(newValue);

	if(triggerSlideEvent === true) {
		this._trigger('slide', newValue);
	}
	if( (oldValue !== newValue) && (triggerChangeEvent === true) ) {
		this._trigger('change', {
			oldValue: oldValue,
			newValue: newValue
		});
	}

	return this;
};

Slider.prototype._layout = function() {
	var positionPercentages;

	if(this.options.reversed) {
		positionPercentages = [ 100 - this.percentage[0], this.percentage[1] ];
	} else {
		positionPercentages = [ this.percentage[0], this.percentage[1] ];
	}

	this.handle1.style[this.stylePos] = positionPercentages[0]+'%';
	this.handle2.style[this.stylePos] = positionPercentages[1]+'%';

	/* Position ticks and labels */
	if (Array.isArray(this.options.ticks) && this.options.ticks.length > 0) {
		var maxTickValue = Math.max.apply(Math, this.options.ticks);
		var minTickValue = Math.min.apply(Math, this.options.ticks);

		var styleSize = this.options.orientation === 'vertical' ? 'height' : 'width';
		var styleMargin = this.options.orientation === 'vertical' ? 'marginTop' : 'marginLeft';
		var labelSize = this.size / (this.options.ticks.length - 1);

		if (this.tickLabelContainer) {
			var extraMargin = 0;
			if (this.options.ticks_positions.length === 0) {
				this.tickLabelContainer.style[styleMargin] = -labelSize/2 + 'px';
				extraMargin = this.tickLabelContainer.offsetHeight;
			} else {
				/* Chidren are position absolute, calculate height by finding the max offsetHeight of a child */
				for (i = 0 ; i < this.tickLabelContainer.childNodes.length; i++) {
					if (this.tickLabelContainer.childNodes[i].offsetHeight > extraMargin) {
						extraMargin = this.tickLabelContainer.childNodes[i].offsetHeight;
					}
				}
			}
			if (this.options.orientation === 'horizontal') {
				this.sliderElem.style.marginBottom = extraMargin + 'px';
			}
		}
		for (var i = 0; i < this.options.ticks.length; i++) {

			var percentage = this.options.ticks_positions[i] ||
				100 * (this.options.ticks[i] - minTickValue) / (maxTickValue - minTickValue);

			this.ticks[i].style[this.stylePos] = percentage + '%';

			/* Set class labels to denote whether ticks are in the selection */
			this._removeClass(this.ticks[i], 'in-selection');
			if (!this.options.range) {
				if (this.options.selection === 'after' && percentage >= positionPercentages[0]){
					this._addClass(this.ticks[i], 'in-selection');
				} else if (this.options.selection === 'before' && percentage <= positionPercentages[0]) {
					this._addClass(this.ticks[i], 'in-selection');
				}
			} else if (percentage >= positionPercentages[0] && percentage <= positionPercentages[1]) {
				this._addClass(this.ticks[i], 'in-selection');
			}

			if (this.tickLabels[i]) {
				// MODIF MAIF
				//this.tickLabels[i].style[styleSize] = labelSize + 'px';

				if (this.options.ticks_positions[i] !== undefined) {
					this.tickLabels[i].style.position = 'absolute';
					this.tickLabels[i].style[this.stylePos] = this.options.ticks_positions[i] + '%';
					this.tickLabels[i].style[styleMargin] = -labelSize/2 + 'px';
				}
			}
		}
	}

	if (this.options.orientation === 'vertical') {
		this.trackLow.style.top = '0';
		this.trackLow.style.height = Math.min(positionPercentages[0], positionPercentages[1]) +'%';

		this.trackSelection.style.top = Math.min(positionPercentages[0], positionPercentages[1]) +'%';
		this.trackSelection.style.height = Math.abs(positionPercentages[0] - positionPercentages[1]) +'%';

		this.trackHigh.style.bottom = '0';
		this.trackHigh.style.height = (100 - Math.min(positionPercentages[0], positionPercentages[1]) - Math.abs(positionPercentages[0] - positionPercentages[1])) +'%';
	} else {
		this.trackLow.style.left = '0';
		this.trackLow.style.width = Math.min(positionPercentages[0], positionPercentages[1]) +'%';

		this.trackSelection.style.left = Math.min(positionPercentages[0], positionPercentages[1]) +'%';
		this.trackSelection.style.width = Math.abs(positionPercentages[0] - positionPercentages[1]) +'%';

		this.trackHigh.style.right = '0';
		this.trackHigh.style.width = (100 - Math.min(positionPercentages[0], positionPercentages[1]) - Math.abs(positionPercentages[0] - positionPercentages[1])) +'%';

        var offset_min = this.tooltip_min.getBoundingClientRect();
        var offset_max = this.tooltip_max.getBoundingClientRect();

        if (offset_min.right > offset_max.left) {
            this._removeClass(this.tooltip_max, 'top');
            this._addClass(this.tooltip_max, 'bottom');
            this.tooltip_max.style.top = 18 + 'px';
        } else {
            this._removeClass(this.tooltip_max, 'bottom');
            this._addClass(this.tooltip_max, 'top');
            this.tooltip_max.style.top = this.tooltip_min.style.top;
        }
	}

	var formattedTooltipVal;

	if (this.options.range) {
		formattedTooltipVal = this.options.formatter(this.options.value);
		this._setText(this.tooltipInner, formattedTooltipVal);
		this.tooltip.style[this.stylePos] = (positionPercentages[1] + positionPercentages[0])/2 + '%';

		if (this.options.orientation === 'vertical') {
			this._css(this.tooltip, 'margin-top', -this.tooltip.offsetHeight / 2 + 'px');
		} else {
			this._css(this.tooltip, 'margin-left', -this.tooltip.offsetWidth / 2 + 'px');
		}

		if (this.options.orientation === 'vertical') {
			this._css(this.tooltip, 'margin-top', -this.tooltip.offsetHeight / 2 + 'px');
		} else {
			this._css(this.tooltip, 'margin-left', -this.tooltip.offsetWidth / 2 + 'px');
		}

		var innerTooltipMinText = this.options.formatter(this.options.value[0]);
		this._setText(this.tooltipInner_min, innerTooltipMinText);

		var innerTooltipMaxText = this.options.formatter(this.options.value[1]);
		this._setText(this.tooltipInner_max, innerTooltipMaxText);

		this.tooltip_min.style[this.stylePos] = positionPercentages[0] + '%';

		if (this.options.orientation === 'vertical') {
			this._css(this.tooltip_min, 'margin-top', -this.tooltip_min.offsetHeight / 2 + 'px');
		} else {
			this._css(this.tooltip_min, 'margin-left', -this.tooltip_min.offsetWidth / 2 + 'px');
		}

		this.tooltip_max.style[this.stylePos] = positionPercentages[1] + '%';

		if (this.options.orientation === 'vertical') {
			this._css(this.tooltip_max, 'margin-top', -this.tooltip_max.offsetHeight / 2 + 'px');
		} else {
			this._css(this.tooltip_max, 'margin-left', -this.tooltip_max.offsetWidth / 2 + 'px');
		}
	} else {
		formattedTooltipVal = this.options.formatter(this.options.value[0]);
		this._setText(this.tooltipInner, formattedTooltipVal);

		this.tooltip.style[this.stylePos] = positionPercentages[0] + '%';
		if (this.options.orientation === 'vertical') {
			this._css(this.tooltip, 'margin-top', -this.tooltip.offsetHeight / 2 + 'px');
		} else {
			this._css(this.tooltip, 'margin-left', -this.tooltip.offsetWidth / 2 + 'px');
		}
	}
}
$(document).ready(function(){
	$('.solutions .panel>a').on('click', function(event){
		var elm = $(event.target);
		if(elm.attr('aria-expanded') != 'true')
			$(document).scrollTop(elm.closest('.solutions').offset().top);
	});
});
(function($){
	$.enableTriggerTableauSlide = function(){
		var viewport = ResponsiveBootstrapToolkit;
		$('.tableau-slide').each(function(){
			var table = $(this);
			var rowsChilds = [];
			$(this).find('.garantie .gar').each(function(){
				var index = $(this).index();
				var $this = $(this);
				rowsChilds.push($this.find('.gar-val').get(0));
				table.find('.vals').eq(index).find('.val').each(function(){
					var $this = $(this);
					rowsChilds.push($this.get(0));
				});

			});
			$(rowsChilds).matchHeight({byRow:true});

			$(window).resize(function(){
				table.executeRsizeTableauSlide();
			});

			table.executeRsizeTableauSlide();
		});
	}

	$.fn.executeRsizeTableauSlide = function(){
		return this.each(function(){
			var viewport = ResponsiveBootstrapToolkit;
			var table = $(this);
			if(viewport.is('sm')) {
				table.find('.container-vals').attr('style', '');
				if(table.hasClass('vals-tablet-scrollable')){
					var widthTotal = 0;
					table.find('.container-vals header .col').each(function(){
						widthTotal += $(this).hasClass('col-sm-size-2') ? 100 : 220;
					});
					table.find('.container-vals').width(widthTotal);
				}
			}
			else if(viewport.is('xs')){
				table.find('.container-vals').width(table.find('.container-vals header .col').size() * 115);
			}
		});
	}

	$.enableMobileTriggerTableauSlide = function(){
		var viewport = ResponsiveBootstrapToolkit;
		$('.tableau-slide').each(function(){
			var table = $(this);
			var vals_scroll = table.find('.container-vals-scroll');

			table.find('.mobile-header .collapse-container a').off('click').on('click', function(){
				var li = $(this).parent();
				var ul = li.parent();
				var idx = li.index();
				var container = $(this).closest('.collapse-container').parent();
				var a = container.prev();
				
				ul.find('li.active').removeClass('active background-univers');
				li.addClass('active background-univers');
				a.find('.inactive .val').html(li.find('a').html()).find('.maificon').remove();

				var vals = table.find('.container-vals .col');
				vals.removeClass('active');
				var elm;
				table.find('.container-vals .val').each(function(){
					elm = $(this).find('.col').eq(idx).addClass('active');
				});
				$.disableMobileTriggerTableauSlideScroll();
				vals_scroll.animate({
					scrollLeft : elm.position().left + vals_scroll.scrollLeft()
				}, 200, function(){
					$.enableMobileTriggerTableauSlideScroll();
				});

				container.collapse('hide');
			});
		});
	}

	$.enableMobileTriggerTableauSlideScroll = function(){
		$('.tableau-slide').each(function(){
			var table = $(this);
			var vals_scroll = table.find('.container-vals-scroll');
			var a = table.find('.mobile-header>a');
			vals_scroll.off('scroll').on('scroll', function(){
				table.find('.vals:not(.vals-header):first .col').each(function(){
					var widthVisible = $(this).position().left < 0 ? $(this).outerWidth(true) + $(this).position().left : vals_scroll.width() - $(this).position().left;
					if(widthVisible > vals_scroll.width() / 2){
						var id = $(this).index();
						var vals = table.find('.container-vals .col');
						vals.removeClass('active');
						table.find('.container-vals .val').each(function(){
							$(this).find('.col').eq(id).addClass('active');
						});
						var lis = table.find('.mobile-header .collapse-container li');
						lis.filter('.active').removeClass('active background-univers');
						lis.eq(id).addClass('active background-univers');
						a.find('.inactive .val').html(lis.eq(id).find('a').html()).find('.maificon').remove();
					}
				});
			});
		});
	}

	$.disableMobileTriggerTableauSlideScroll = function(){
		$('.tableau-slide').each(function(){
			var table = $(this);
			var vals_scroll = table.find('.container-vals-scroll');
			vals_scroll.off('scroll');
		});
	}
})(jQuery);

$(document).ready(function(){
	$.enableTriggerTableauSlide();
	$.enableMobileTriggerTableauSlide();
	$.enableMobileTriggerTableauSlideScroll();
});
(function($) {
	var stoTuile;

	$.scrollToTop = function(){
		$('html, body').animate({scrollTop:0}, 500);
	}

	$.fn.toggleTuile = function(){
		return this.each(function(){
			var tuile = $(this).closest('.tuile');

			$('.tuile.open').not(tuile).toggleTuile();
			clearTimeout(stoTuile);

			var heightRecto, heightVerso;
			if(tuile.data('heightRecto') !== undefined && tuile.data('heightVerso') !== undefined){
				heightRecto = tuile.data('heightRecto');
				heightVerso = tuile.data('heightVerso');
			}
			else{
				heightRecto = tuile.find('.recto').height();
				heightVerso = tuile.find('.verso').height();

				tuile.data('heightRecto', heightRecto);
				tuile.data('heightVerso', heightVerso);
			}

			//console.log(heightRecto, heightVerso);

			if(tuile.hasClass('open')){
				tuile.find('.tuile-container').height(heightVerso);
				tuile.addClass('close').removeClass('open');
				tuile.find('.tuile-container').height(heightRecto);

				stoTuile = setTimeout(function(){
					tuile.find('.tuile-container').attr('style', '');
					heightRecto = tuile.find('.recto').height();
					tuile.data('heightRecto', heightRecto);
				}, 500);
			}
			else{
				heightRecto = tuile.find('.recto').height();
				tuile.data('heightRecto', heightRecto);
				tuile.find('.tuile-container').height(heightRecto);
				tuile.addClass('open').removeClass('close');
				tuile.find('.tuile-container').height(heightVerso);
			}
		});
	}

	$.fn.toggleSwitch = function(){
		return this.each(function(){
			var switchC = $(this).closest('.switch');

			if(switchC.find('input:first').is(':checked'))
				switchC.find('input:last').prop('checked', true).change();
			else
				switchC.find('input:first').prop('checked', true).change();
		})
	}

	$.fn.getCursorPosition = function() {
        var input = this.get(0);
        if (!input) return; // No (input) element found
        if (document.selection) {
            // IE
           input.focus();
        }
        return 'selectionStart' in input ? input.selectionStart:'' || Math.abs(document.selection.createRange().moveStart('character', -input.value.length));
     }

    $.sliderValeurHandler = function(){
     	$('.slider-valeur:not(.slider-valeur-replaced)').each(function() {
     		$(this).addClass('slider-valeur-replaced');
			$(this).find('input').slider({
				formatter : function(value) {
					return '';
				}
			}).on('change', function(event){
				initChangeSliderFunc(this, event.value.newValue);
			});

			var initLabelPositions = function(elm){
				var labelsnb = $(elm).closest('.slider-valeur').find('.slider-tick-label').size();
				if(labelsnb > 0){
					if(labelsnb == 4)
					{
						var labels = $(elm).closest('.slider-valeur').find('.slider-tick-label');
						labels.eq(0).css('width', '20%');
						labels.eq(1).css('width', '30%');
						labels.eq(2).css('width', '30%');
						labels.eq(3).css('width', '20%');
					}
					else
						$(elm).closest('.slider-valeur').find('.slider-tick-label').css('width', 100 / labelsnb + '%');
				}
			}
			
			initLabelPositions(this);

			var initChangeSliderFunc = function(elm, value){
				$(elm).closest('.slider-valeur').find('.slider-tick-label.active').removeClass('active color-univers');
				var idx = $(elm).closest('.slider-valeur').find('input').data('slider-ticks').indexOf(value);

				if(idx != -1)
					$(elm).closest('.slider-valeur').find('.slider-tick-label').eq(idx).addClass('active color-univers');
			}

			initChangeSliderFunc(this, $(this).find('input').data('slider-value'));
		});
    }

    $.sliderSlotsHandler = function(){
    	$('.slider-slots:not(.slider-slots-replaced)').each(function() {
     		$(this).addClass('slider-slots-replaced');
			$(this).find('input').slider({
				formatter : function(value) {
					return '';
				}
			});
		});
    }

    $.toggleSwitchHandler = function(){
    	$('.maif-form .switch .toggle-switch').off('click').on('click', function(){
			$(this).toggleSwitch();
		});
	}

	$.openTabExternal = function(){
		var hash = document.location.hash;
		if ($(hash).size() > 0 && $('[data-target="' + hash + '"]').parent().is('.nav-tabs')) {
		    $('[data-target="' + hash + '"]').tab('show');
		    $(hash).find('.panel-collapse').collapse('show');
		    if($('[data-target="' + hash + '"]').closest('.nav-tabs').offset().top == 0)
		    	$('body').animate({scrollTop: $(hash).find('.panel-collapse').prev().offset().top}, 200);
		    else
		    	$('body').animate({scrollTop: $('[data-target="' + hash + '"]').closest('.nav-tabs').offset().top}, 200);
		}
	}

	$.openTabInternal = function(id){
		if ($(id).size() > 0 && $('[data-target="' + id + '"]').parent().is('.nav-tabs')) {
		    $('[data-target="' + id + '"]').tab('show');
		    $(id).find('.panel-collapse').collapse('show');

		    if(ResponsiveBootstrapToolkit.current() == 'xs')
		    	$('body').animate({scrollTop: $(id).find('.panel-collapse').prev().offset().top}, 200);
		    else
		    	$('body').animate({scrollTop: $('[data-target="' + id + '"]').closest('.nav-tabs').offset().top}, 200);
		}
	}

	$.fn.scrollToElement = function(decalage){
		return this.each(function(){
			if($(this).size() > 0){
				var offset_top = $(this).eq(0).offset().top;
				decalage = parseInt(decalage, 10);
				if(!isNaN(decalage))
					offset_top += decalage;
				$('html,body').animate({
					scrollTop : offset_top
				}, 200);
			}
		})
	}

	$.fn.closeTabs = function()
	{
		return this.each(function(){
			$(this).find('.active').removeClass('active');
		});
	}

	$.menuTogglePasswordVisibility = function(){
        if($('#menu-password-field').is('input[type="password"]'))
            $('#menu-password-field').attr('type', 'text');
        else
            $('#menu-password-field').attr('type', 'password');
    }
})(jQuery);

$(document).ready(function(){
	$('.selectpicker').selectpicker();
	
	$('[data-ride="carousel"]').hammer().on('swipeleft', function(){
		$(this).carousel('next'); 
	});
	$('[data-ride="carousel"]').hammer().on('swiperight', function(){
		$(this).carousel('prev'); 
	});

	$('.carousel-conseils-prevention .items-container').slick({
		centerMode: true,
		centerPadding: '60px',
		slidesToShow: 1,
		variableWidth: true,
		appendArrows: $('.carousel-conseils-prevention .container-arrows'),
		prevArrow: '<a href="#" class="btn btn-bevel-left"><i class="maificon maificon-arrow-left"></i></a>',
		nextArrow: '<a href="#" class="btn btn-bevel-right"><i class="maificon maificon-arrow-right"></i></a>',
		responsive: [
			{
				breakpoint: 768,
				settings: 'unslick'
			}
		]
	});

	$('.homepage-cp-applications .nav.nav-tabs').on('show.bs.tab', function(event){
		$(this).addClass('active');
		$(event.target).on('click.maif', function(event){
			$('.homepage-cp-applications').closeTabs();
			$('.homepage-cp-applications .nav.nav-tabs li>a').off('click.maif');
			event.stopPropagation();
			return false;
		});

		if(event.relatedTarget)
			$(event.relatedTarget).off('click.maif');
	}).on('hide.bs.tab', function(event){
		$(this).removeClass('active');
	});

	$('body').on('touchstart', function(event) {
		if($('.hidden-xs').css('display') == 'block') {
			// code that makes element or parent slide or otherwise move out from under mouse.
			if($(event.target).is('ul#nav-univers>li>a')){
				$('.hovered').removeClass('hovered');
				if($(event.target).closest('#nav-univers').size() == 0) {
					var link = $("#nav-univers>li:hover");
					link.clone(true).insertAfter(link);
					link.remove();
				} else {
					if(!$(event.target).hasClass('hovered')){
						$(event.target).closest('li').addClass('hovered').parent().addClass('hovered');
						$(event.target).addClass('hovered');
						return false;
					}
				}
			}
		}
	});

	// Gestion des erreurs
	if($('p.error-message').size() > 0) {
		if($('p.error-message:first').closest('.has-error').size() == 1) {
			$('html,body').animate({
				scrollTop : $('p.error-message:first').closest('.has-error').offset().top
			}, 200);
			
		}
		else {
			$('html,body').animate({
				scrollTop : $('p.error-message:first').offset().top
			}, 200);
		}
	}

	if($('.champ-texte .unite').size() > 0) {
		$('.champ-texte .unite').each(function(){
			var $this = $(this);
			$this.prev().css('padding-right', $this.outerWidth());
		});
	}

	$.sliderValeurHandler();
	$.toggleSwitchHandler();
	$.openTabExternal();
});
(function($){
	$.pageArticleNavigation = function(){
		$(window).on('scroll', function(){
			if($('.mod-edito-onglet.visible-xs').size() > 0) {
				var s = $(window).scrollTop(), d = $(document).height(), c = $(window).height();
				var scrollPercent = (s / (d-c)) * 100;

				$('.mod-edito-onglet.visible-xs:first .progress').css('width', 100 - scrollPercent + '%');

				var positionOnglets = $(window).scrollTop() - $('.mod-edito-onglet.visible-xs:first').offset().top;

				if(positionOnglets > 0)
					$('.mod-edito-onglet.visible-xs:first').addClass('affix-top');
				else
					$('.mod-edito-onglet.visible-xs:first').removeClass('affix-top');
			}
		});
	}
})(jQuery);

$(document).ready(function(){
	$.pageArticleNavigation();
});
(function($){
	/** Formules **/
	var stoPPFormule1, stoPPFormule2;

	$.activatePPFormuleCollapseHandler = function(viewport){
		$('.pp-formule .collapse-handler').off('click').on('click', function(event){
			$('.pp-formule .collapse-handler').off('click').on('click', function(){return false});
			$('.pp-formule .collapse-content, .pp-formule .collapse-content-option').addClass('collapsing');

			if(viewport.is('>=md')) {
				var formuleContainer = $(this).closest('.pp-formule');

				if(formuleContainer.data('active') === undefined || formuleContainer.data('active') === false)
					$('.pp-formule').data('active', true);
				else
					$('.pp-formule').data('active', false);

				formuleContainer.toggleClass('active-formule');

				if(formuleContainer.data('active') === true){
					$('.pp-formule').addClass('active').find('.collapse-content-container').matchHeight();
					$('.pp-formule').find('.collapse-content').height($('.pp-formule').find('.collapse-content-container').outerHeight() - 2);
					$('.pp-formule').find('.collapse-content-option').height($('.pp-formule').find('.collapse-content-option-container').outerHeight());
				}
				else{
					$('.pp-formule').find('.collapse-content').css('height', '0');
					$('.pp-formule').find('.collapse-content-option').css('height', '0');
					clearTimeout(stoPPFormule1);
					stoPPFormule1 = setTimeout(function(){
						$('.pp-formule').removeClass('active');
					}, 350);
				}

				clearTimeout(stoPPFormule2);
				stoPPFormule2 = setTimeout(function(){
					$('.pp-formule .collapse-content, .pp-formule .collapse-content-option').removeClass('collapsing');
					$.activatePPFormuleCollapseHandler(viewport);
				}, 350);
			}
			else {
				var formuleContainer = $(this).closest('.pp-formule');

				formuleContainer.toggleClass('active-formule');
				if(formuleContainer.hasClass('active-formule')){
					formuleContainer.find('.collapse-content').height(formuleContainer.find('.collapse-content-container').outerHeight() - 2);
					formuleContainer.find('.collapse-content-option').height(formuleContainer.find('.collapse-content-option-container').outerHeight());
				}
				else{
					formuleContainer.find('.collapse-content').css('height', '0');
					formuleContainer.find('.collapse-content-option').css('height', '0');
				}

				clearTimeout(stoPPFormule2);
				stoPPFormule2 = setTimeout(function(){
					$('.pp-formule .collapse-content, .pp-formule .collapse-content-option').removeClass('collapsing');
					$.activatePPFormuleCollapseHandler(viewport);
				}, 350);
			}
			return false;
		});
	}

	$.reInitPPFormuleCollapseHandler = function(viewport){
		$('.pp-formule').removeData('active').removeClass('active active-formule');
		$('.pp-formule .collapse-content, .pp-formule .collapse-content-container, .pp-formule .collapse-content-option, .pp-formule .collapse-content-option-container').removeAttr('style');
		$.activatePPFormuleCollapseHandler(viewport);
	}

	/** Tableau comparatif **/
	$.activatePPTableauComparatifHandler = function(){
		$('.pp-tableau-comparatif a.garantie[data-toggle="collapse"]').on('click', function(){
			$('.last-clicked').removeClass('last-clicked');
			$(this).addClass('last-clicked');
			$('.pp-tableau-comparatif .collapse.in').collapse('hide');
			$(this).next().on('shown.bs.collapse', function(){
				if(!$(this).prev().hasClass('last-clicked'))
					$(this).collapse('hide');
			});
		});
	}
})(jQuery);

var viewportCurrent;

$(document).ready(function(){
	if($('.pp-formule').size() > 0){
		$.activatePPFormuleCollapseHandler(ResponsiveBootstrapToolkit);

		viewportCurrent = ResponsiveBootstrapToolkit.current();

		$(window).resize(function(){
			if(!ResponsiveBootstrapToolkit.is(viewportCurrent)){
				viewportCurrent = ResponsiveBootstrapToolkit.current();
				$.reInitPPFormuleCollapseHandler(ResponsiveBootstrapToolkit);
			}
	    });
	}

	if($('.pp-tableau-comparatif').size() > 0){
		$.activatePPTableauComparatifHandler();
	}
});
(function($){
    $.activateDatepicker = function() {
        if (!$.support.inputDate) {
            $('.form-date:not(.form-date-lointaine) input[type="date"]').each(function() {
                var isFormatUs = $(this).val().split('-').length > 1;

                if(isFormatUs) {
                    var dateUs = $(this).val().split('-');
                    if(dateUs[0] !== undefined) {
                        var newFormat = dateUs[2] + '/' + dateUs[1] + '/' + dateUs[0];
                        $(this).val(newFormat);
                    }
                }

                var params = {
                    todayBtn: 'linked',
                    language: 'fr',
                    autoclose: true,
                    todayHighlight: true,
                    toggleActive: false
                };

                if($(this).attr('min') !== undefined) {
                    var dateUs = $(this).attr('min').split('-');
                    if(dateUs[0] !== undefined) {
                        var newFormat = dateUs[2] + '/' + dateUs[1] + '/' + dateUs[0];
                        params['startDate'] = newFormat;
                    }
                }

                if($(this).attr('max') !== undefined) {
                    var dateUs = $(this).attr('max').split('-');
                    if(dateUs[0] !== undefined) {
                        var newFormat = dateUs[2] + '/' + dateUs[1] + '/' + dateUs[0];
                        params['endDate'] = newFormat;
                    }
                }

                $(this).datepicker(params);
            });
        }
        else{
            // Pour les navigateur ou le comportement est d'appeler le onchange lors d'une reconnaissance de date (et non lors d'un changement de champ)
            $('.form-date:not(.form-date-lointaine) input[type="date"]').each(function() {
                var $this = $(this);
                $this.data('onChange', $this.attr('onchange')).removeAttr('onchange').off('change');
                $this.on('blur', function(){
                    var func = function(){
                        eval(this.data('onChange'));
                    }

                    func.call($this);
                });
            });
        }

        if (!$.support.inputMonth) {
            $('.form-date:not(.form-date-lointaine) input[type="month"]').each(function() {
                var isFormatUs = $(this).val().split('-').length > 1;

                if(isFormatUs) {
                    var dateUs = $(this).val().split('-');
                    if(dateUs[0] !== undefined) {
                        var newFormat = dateUs[1] + '/' + dateUs[0];
                        $(this).val(newFormat);
                    }
                }

                var params = {
                    format: 'mm/yyyy',
                    todayBtn: 'linked',
                    language: 'fr',
                    autoclose: true,
                    todayHighlight: true,
                    toggleActive: false,
                    minViewMode: 1
                };

                if($(this).attr('min') !== undefined) {
                    var dateUs = $(this).attr('min').split('-');
                    if(dateUs[0] !== undefined) {
                        var newFormat = dateUs[1] + '/' + dateUs[0];
                        params['startDate'] = newFormat;
                    }
                }

                if($(this).attr('max') !== undefined) {
                    var dateUs = $(this).attr('max').split('-');
                    if(dateUs[0] !== undefined) {
                        var newFormat = dateUs[1] + '/' + dateUs[0];
                        params['endDate'] = newFormat;
                    }
                }


                $(this).datepicker(params);
            });
        }
        else{
            // Pour les navigateur ou le comportement est d'appeler le onchange lors d'une reconnaissance de date (et non lors d'un changement de champ)
            $('.form-date:not(.form-date-lointaine) input[type="month"]').each(function() {
                var $this = $(this);
                $this.data('onChange', $this.attr('onchange')).removeAttr('onchange').off('change');
                $this.on('blur', function(){
                    var func = function(){
                        eval(this.data('onChange'));
                    }

                    func.call($this);
                });
            });
        }
    }

    $.activateDatepickerLointain = function() {
        $('.form-date.form-date-lointaine:not(.form-date-lointaine-replaced)').each(function(){
            var $this = $(this);
            $this.addClass('form-date-lointaine-replaced');
            var typeField = 'date';
            var field1 = $this.find('input[type="date"]');
            if(field1.size() == 0){
                field1 = $this.find('input[type="month"]');
                typeField = 'month';
            }
            var value = field1.val();
            var field2 = $('<input type="text" />');
            if(field1.hasClass('input-large'))
                field2.addClass('input-large');
            if(field1.prop('disabled'))
                field2.prop('disabled', true);
            var pad = "00";
            field2.appendTo($this);

            field1.off('focusout').on('focusout', function(){
                return false;
            }).addClass('visible-xs');
            field2.addClass('hidden-xs');

            field2.on('focusout', function(){
                var evt = document.createEvent("Event");
                evt.initEvent("blur", true, true); 
                field1.get(0).dispatchEvent(evt);
            });

            if(typeField == 'date'){
                var dateInit = ['JJ', 'MM', 'AAAA'];

                var regexpDate = /^(?:(?:(\d{4})[\-](0?[1-9]|1[012])[\-](0?[1-9]|[12][0-9]|3[01]))|(?:(0?[1-9]|[12][0-9]|3[01])[\/](0?[1-9]|1[012])[\/](\d{4})))$/;
                if(value.match(regexpDate)){
                    var dateStr = value.match(regexpDate);
                    if(dateStr[0].match(/\-/)){
                        date = new Date(dateStr[1], dateStr[2]-1, dateStr[3]);
                        // console.log(dateStr[1], dateStr[2]-1, dateStr[3]);
                    }
                    else{
                        date = new Date(dateStr[6], dateStr[5]-1, dateStr[4]);
                        // console.log(dateStr[6], dateStr[5]-1, dateStr[4]);
                    }
                    dateInit = [
                        pad.substring(0, pad.length - date.getDate().toString().length) + date.getDate(),
                        pad.substring(0, pad.length - (date.getMonth() + 1).toString().length) + (date.getMonth() + 1),
                        date.getFullYear()
                    ];
                }

                field2.val(dateInit.join('/'));
                var lastPos = field2.caret();

                field2.on('focus', function(event){
                    var elm = $(this);
                    elm.caret(0, 2);
                    lastPos = elm.caret();
                });


                field2.on('click keypress keydown', function(event){
                    var elm = $(this);

                    if(event.type == 'click'){
                        lastPos = elm.caret();
                        if(lastPos.start >= 0 && lastPos.start <= 2)
                            elm.caret(0, 2);
                        else if(lastPos.start >= 3 && lastPos.start <= 5)
                            elm.caret(3, 5);
                        else if(lastPos.start >= 6 && lastPos.start <= 10)
                            elm.caret(6, 10);
                        lastPos = elm.caret();
                    }
                    else if(event.type == 'keypress'){
                        var kCode = event.keyCode ? event.keyCode : event.which;
                        var character = String.fromCharCode(kCode);

                        // Si 0123456789
                        if(kCode >= 48 && kCode <= 57){
                            // Day
                            if((lastPos.start >= 0 && lastPos.start <= 2) || (lastPos.start == 0 && lastPos.end == 0)){
                                if(dateInit[0][0] == 'J' || (lastPos.start == 0 && lastPos.end == 2) || (lastPos.start == 0 && lastPos.end == 0)){
                                    // 0 à 3
                                    if(character >= 0 && character <= 3){
                                        dateInit[0] = pad.substring(0, 1) + character;
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(2, 2);
                                    }
                                    else{
                                        dateInit[0] = pad.substring(0, 1) + character;
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(3, 5);
                                    }
                                }
                                else{
                                    var maxDay = 31;
                                    if(dateInit[0][1] + character > maxDay)
                                        dateInit[0] = maxDay;
                                    else
                                        dateInit[0] = dateInit[0][1] + character;
                                    field2.val(dateInit.join('/')).trigger('change');
                                    elm.caret(3, 5);
                                }
                            }
                            // Month
                            else if(lastPos.start >= 3 && lastPos.start <= 5){
                                if(dateInit[1][0] == 'M' || (lastPos.start == 3 && lastPos.end == 5)){
                                    // 0 à 1
                                    if(character >= 0 && character <= 1){
                                        dateInit[1] = pad.substring(0, 1) + character;
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(5, 5);
                                    }
                                    else{
                                        dateInit[1] = pad.substring(0, 1) + character;
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(6, 10);
                                    }
                                }
                                else{
                                    var maxMonth = 12;
                                    if(dateInit[1][1] + character > maxMonth)
                                        dateInit[1] = maxMonth;
                                    else
                                        dateInit[1] = dateInit[1][1] + character;
                                    field2.val(dateInit.join('/')).trigger('change');
                                    elm.caret(6, 10);
                                }
                            }
                            // Year
                            else if(lastPos.start >= 6 && lastPos.start <= 10){
                                if(dateInit[2] == 'AAAA')
                                    dateInit[2] = '';
                                
                                if(dateInit[2].length == 4 || (lastPos.start != lastPos.end))
                                {
                                    if(lastPos.start != 10)
                                        dateInit[2] = '';
                                    else
                                        return false;
                                }

                                dateInit[2] = dateInit[2] + "" + character;
                                field2.val(dateInit.join('/')).trigger('change');
                                elm.caret(10, 10);
                            }

                            lastPos = elm.caret();
                        }
                        // Slash
                        else if(kCode == 47){
                            if(lastPos.start >= 0 && lastPos.start <= 2)
                                elm.caret(3, 5);
                            else if(lastPos.start >= 3 && lastPos.start <= 5)
                                elm.caret(6, 10);
                            lastPos = elm.caret();
                        }
                        
                        if(kCode != 9)
                            return false;
                    }
                    else if(event.type == 'keydown'){
                        var kCode = event.keyCode ? event.keyCode : event.which;

                        if(kCode == 8 || kCode == 46){ // delete
                            if(lastPos.start >= 0 && lastPos.start <= 2){
                                dateInit[0] = 'JJ';
                                field2.val(dateInit.join('/')).trigger('change');
                                elm.caret(0, 2);
                                lastPos = elm.caret();
                                return false;
                            }
                            else if(lastPos.start >= 3 && lastPos.start <= 5){
                                if(dateInit[1] == 'MM'){
                                    elm.caret(0, 2);
                                }
                                else{
                                    dateInit[1] = 'MM';
                                    field2.val(dateInit.join('/')).trigger('change');
                                    elm.caret(3, 5);
                                }
                                lastPos = elm.caret();
                                return false;
                            }
                            else if(lastPos.start >= 6 && lastPos.start <= 10){
                                if(dateInit[2] == 'AAAA'){
                                    elm.caret(3, 5);
                                    lastPos = elm.caret();
                                    return false;
                                }
                                else{
                                    dateInit[2] = dateInit[2].substring(0, dateInit[2].length - 1);
                                    field2.val(dateInit.join('/')).trigger('change');
                                    if(dateInit[2].length == 0){
                                        dateInit[2] = 'AAAA';
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(6, 10);
                                        lastPos = elm.caret();
                                    }
                                    return false;
                                }
                            }
                        }
                        else if(kCode == 37){ // arrow left
                            if(lastPos.start >= 6 && lastPos.start <= 10)
                                elm.caret(3, 5);
                            else
                                elm.caret(0, 2);
                            lastPos = elm.caret();
                            return false;
                        }
                        else if(kCode == 39){ // arrow right
                            if(lastPos.start >= 0 && lastPos.start <= 2)
                                elm.caret(3, 5);
                            else
                                elm.caret(6, 10);
                            lastPos = elm.caret();
                            return false;
                        }
                        else if(kCode == 38){ // arrow top
                            elm.caret(0, 2);
                            lastPos = elm.caret();
                            return false;
                        }
                        else if(kCode == 40){ // arrow bottom
                            elm.caret(6, 10);
                            lastPos = elm.caret();
                            return false;
                        }
                    }
                });

                field1.on('change', function(){
                    var viewport = ResponsiveBootstrapToolkit;
                    if(viewport.is('<sm')) {
                        
                    }
                    // console.log('Field1 : ' + $(this).val());
                });

                field2.on('change', function(){
                    var viewport = ResponsiveBootstrapToolkit;
                    if(viewport.is('>=sm')) {
                        var vals = $(this).val().split('/');
                        field1.val(vals[2] + '-' + vals[1] + '-' + vals[0]).trigger('change');
                    }
                });
            }
            else if(typeField == 'month')
            {
                var dateInit = ['MM', 'AAAA'];

                var regexpDate = /^(\d{4})[\-](0?[1-9]|1[012])$/;
                if(value.match(regexpDate)){
                    var dateStr = value.match(regexpDate);
                    date = new Date(dateStr[1], dateStr[2]-1);
                    dateInit = [
                        pad.substring(0, pad.length - (date.getMonth() + 1).toString().length) + (date.getMonth() + 1),
                        date.getFullYear()
                    ];
                }

                field2.val(dateInit.join('/'));
                var lastPos = field2.caret();

                field2.on('focus', function(event){
                    if(event.relatedTarget !== null){
                        var elm = $(this);
                        elm.caret(0, 2);
                        lastPos = elm.caret();
                    }
                });

                field2.on('click keypress keydown', function(event){
                    var elm = $(this);

                    if(event.type == 'click'){
                        lastPos = elm.caret();
                        if(lastPos.start >= 0 && lastPos.start <= 2)
                            elm.caret(0, 2);
                        else if(lastPos.start >= 3 && lastPos.start <= 7)
                            elm.caret(3, 7);
                        lastPos = elm.caret();
                    }
                    else if(event.type == 'keypress'){
                        var kCode = event.keyCode ? event.keyCode : event.which;
                        var character = String.fromCharCode(kCode);

                        // Si 0123456789
                        if(kCode >= 48 && kCode <= 57){
                            // Month
                            if((lastPos.start >= 0 && lastPos.start <= 2) || (lastPos.start == 0 && lastPos.end == 0)){
                                if(dateInit[0][0] == 'M' || (lastPos.start == 0 && lastPos.end == 2) || (lastPos.start == 0 && lastPos.end == 0)){
                                    // 0 à 1
                                    if(character >= 0 && character <= 1){
                                        dateInit[0] = pad.substring(0, 1) + character;
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(2, 2);
                                    }
                                    else{
                                        dateInit[0] = pad.substring(0, 1) + character;
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(3, 7);
                                    }
                                }
                                else{
                                    var maxMonth = 12;
                                    if(dateInit[0][1] + character > maxMonth)
                                        dateInit[0] = maxMonth;
                                    else
                                        dateInit[0] = dateInit[0][1] + character;
                                    field2.val(dateInit.join('/')).trigger('change');
                                    elm.caret(3, 7);
                                }
                            }
                            // Year
                            else if(lastPos.start >= 3 && lastPos.start <= 7){
                                if(dateInit[1] == 'AAAA')
                                    dateInit[1] = '';
                                if(dateInit[1].length == 4 || (lastPos.start != lastPos.end))
                                {
                                    if(lastPos.start != 7)
                                        dateInit[1] = '';
                                    else
                                        return false;
                                }

                                dateInit[1] = dateInit[1] + "" + character;
                                field2.val(dateInit.join('/')).trigger('change');
                                elm.caret(7, 7);
                            }

                            lastPos = elm.caret();
                        }
                        // Slash
                        else if(kCode == 47){
                            if(lastPos.start >= 0 && lastPos.start <= 2)
                                elm.caret(3, 7);
                            lastPos = elm.caret();
                        }
                        
                        if(kCode != 9)
                            return false;
                    }
                    else if(event.type == 'keydown'){
                        var kCode = event.keyCode ? event.keyCode : event.which;
                        if(kCode == 8 || kCode == 46){ // delete
                            if(lastPos.start >= 0 && lastPos.start <= 2){
                                dateInit[0] = 'MM';
                                field2.val(dateInit.join('/')).trigger('change');
                                elm.caret(0, 2);
                                lastPos = elm.caret();
                                return false;
                            }
                            else if(lastPos.start >= 3 && lastPos.start <= 7){
                                if(dateInit[1] == 'AAAA'){
                                    elm.caret(0, 2);
                                    lastPos = elm.caret();
                                    return false;
                                }
                                else{
                                    dateInit[1] = dateInit[1].substring(0, dateInit[1].length - 1);
                                    field2.val(dateInit.join('/')).trigger('change');
                                    if(dateInit[1].length == 0){
                                        dateInit[1] = 'AAAA';
                                        field2.val(dateInit.join('/')).trigger('change');
                                        elm.caret(3, 7);
                                        lastPos = elm.caret();
                                    }
                                    return false;
                                }
                            }
                        }
                        else if(kCode == 37){ // arrow left
                            elm.caret(0, 2);
                            lastPos = elm.caret();
                            return false;
                        }
                        else if(kCode == 39){ // arrow right
                            elm.caret(3, 7);
                            lastPos = elm.caret();
                            return false;
                        }
                        else if(kCode == 38){ // arrow top
                            elm.caret(0, 2);
                            lastPos = elm.caret();
                            return false;
                        }
                        else if(kCode == 40){ // arrow bottom
                            elm.caret(3, 7);
                            lastPos = elm.caret();
                            return false;
                        }
                    }
                });

                field1.on('change', function(){
                    var viewport = ResponsiveBootstrapToolkit;
                    if(viewport.is('<sm')) {
                        
                    }
                    // console.log('Field1 : ' + $(this).val());
                });

                field2.on('change', function(){
                    var viewport = ResponsiveBootstrapToolkit;
                    if(viewport.is('>=sm')) {
                        var vals = $(this).val().split('/');
                        field1.val(vals[1] + '-' + vals[0]).trigger('change');
                    }
                });
            }
        });
    }


})(jQuery);

$(document).ready(function() {
    $.activateDatepicker();
    $.activateDatepickerLointain();
});

var clickTouchEvent = 'click';

$(document).ready(function(){
	$('#maif-navigation-toggle, #maif-navigation').on(clickTouchEvent, function(e){
		if($(e.target).is('#maif-navigation') || $(e.target).closest('#maif-navigation-toggle').size() > 0){
			$('#maif-navigation-toggle').toggleClass('collapsed');
			$('#maif-navigation').toggleClass('in');
			$('#maif-navigation ul#nav-univers>li').removeClass('active-mobile');
			$('body').toggleClass('navigation-in');
		}
	});
	
	if(!$('#maif-navigation ul#nav-univers').hasClass('simplified')){
		$('#maif-navigation ul#nav-univers>li>a').on(clickTouchEvent, function(e){
			if($(this).closest('li').is('.espace-perso.login')){
				$('#maif-navigation,#maif-navigation-toggle').addClass('in-espace-perso');
			}
			else if(!$(this).closest('li').is('.espace-perso')){
				$('#maif-navigation ul#nav-univers>li>a').not($(this)).closest('li').removeClass('active-mobile');
				var li = $(this).closest('li');
				li.toggleClass('active-mobile');
				if(li.hasClass('active-mobile'))
					$('#maif-navigation>.container').animate({scrollTop: li.position().top}, 0);
				if($('.hidden-xs').css('display') == 'none')
					return false;
			}
		});
	}
	

	$('.container-espace-perso .back').on(clickTouchEvent, function(e){
		$('#maif-navigation,#maif-navigation-toggle').removeClass('in-espace-perso');
	});

	$('.container-espace-perso .close').on(clickTouchEvent, function(e){		
		$('#maif-navigation,#maif-navigation-toggle').removeClass('in in-espace-perso');
		$('#maif-navigation-toggle').addClass('collapsed');
		$('body').removeClass('navigation-in');
	});


	$(document).on(clickTouchEvent, '#maif-navigation-redux-toggle, #maif-navigation-redux.collapse.in', function(e){
		if($(e.target).is('#maif-navigation-redux') || $(e.target).closest('#maif-navigation-redux-toggle').length > 0){
			if($('#maif-navigation-redux-toggle').hasClass('expanded')){
				$('#maif-navigation-redux-toggle').removeClass('expanded').addClass('collapsed');
			}else{
				$('#maif-navigation-redux-toggle').removeClass('collapsed').addClass('expanded');				
			}
			$('#maif-navigation-redux').toggleClass('in');
			$('#maif-navigation-redux ul#nav-univers>li').removeClass('active-mobile');
			$('body').toggleClass('navigation-in-redux');
		}
	});
	
	$('#maif-navigation-redux ul#nav-univers>li>a').on(clickTouchEvent, function(e){
		e.preventDefault();
		e.stopPropagation();
		
		if($(this).closest('li').is('.espace-perso.login')){
			$('#maif-navigation-redux,#maif-navigation-redux-toggle').addClass('in-espace-perso');
		}
		else if(!$(this).closest('li').is('.espace-perso')){
			$('#maif-navigation-redux ul#nav-univers>li>a').not($(this)).closest('li').removeClass('active-mobile');
			var li = $(this).closest('li');
			li.toggleClass('active-mobile');
			if(li.hasClass('active-mobile')){
				$('#maif-navigation-redux>.container').animate({scrollTop: li.position().top}, 0);
			}
			if($('.hidden-xs').css('display') == 'none')
				return false;
		}
	});
	

	$('.container-espace-perso .back').on(clickTouchEvent, function(e){
		$('#maif-navigation-redux,#maif-navigation-redux-toggle').removeClass('in-espace-perso');
	});

	$('.container-espace-perso .close').on(clickTouchEvent, function(e){
		$('#maif-navigation-redux,#maif-navigation-redux-toggle').removeClass('in in-espace-perso');
		$('#maif-navigation-redux-toggle').addClass('collapsed');
		$('body').removeClass('navigation-in-redux');
		if($('#maif-navigation-redux-toggle').hasClass('expanded')){
			$('#maif-navigation-redux-toggle').removeClass('expanded').addClass('collapsed');
		}else{
			$('#maif-navigation-redux-toggle').removeClass('collapsed').addClass('expanded');				
		}
	});

	var stoMenu;
	var waitingTime = 200;
	$('#nav-univers').off('mouseenter mouseleave click').on('mouseenter', function(){
		var $this = $(this);
		stoMenu = setTimeout(function(){
			$this.addClass('showmenu');
		}, waitingTime);
	}).on('mouseleave', function(){
		clearTimeout(stoMenu);
		$(this).removeClass('showmenu');
	}).on('click', function(){
		$(this).addClass('showmenu');
	});
});
/*
 *
 * Copyright (c) 2010 C. F., Wong (<a href="https://web.archive.org/web/20180811194359/http://cloudgen.w0ng.hk">Cloudgen Examplet Store</a>)
 * Copyright (c) 2016 Valentin Rouet <v.rouet@gmail.com>
 * Licensed under the MIT License:
 * http://www.opensource.org/licenses/mit-license.php
